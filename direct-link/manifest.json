{
  "name": "Direct LINK",
  "version": "1.5",
  "manifest_version": 2,
  "description": "禁用 Google 搜索结果链接安全检查的中转",
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
  },
  "browser_action": {},
  "background": { "scripts": [ "background.js" ] },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "<all_urls>"
  ]
}
